  "description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
  "engines": {
    "node": ">= 14.16 <15"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/jest": "~26.0.20",
    "@types/node": "~14.14.31",
    "@types/unzipper": "^0.10.3",
